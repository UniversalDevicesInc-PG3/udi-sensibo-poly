<editors>
    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>

    <editor id="temperature">
        <range uom="4" min="18" max="30" prec="0" step="1" />
        <range uom="17" min="50" max="88" prec="0" step="1" />
    </editor>

    <editor id="fan">
        <range uom="25" subset="0-6" nls="FAN" />
    </editor>

    <editor id="humidity">
        <range uom="51" min="0" max="100" step="1" />
    </editor>

    <editor id="mode">
        <range uom="25" subset="0-4" nls="MODE" />
    </editor>

    <editor id="power">
        <range uom="25" subset="0,1" nls="POWER" />
    </editor>

    <editor id="seconds">
        <range uom="57" min="0" max="99999" step="1"/>
    </editor>
</editors>
